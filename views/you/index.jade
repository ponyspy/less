extends ../layout

block content
	.content
		.courses_block
			.title
				h2 Расписание
			.scroll
				- for(item in items)
					- if (item.hasOwnProperty(title))
						ol
							each item in items
								li= item.title + ' '
								each date in item.schedule
									b= date
								br
								br
						- break				
					- else
						b У вас нет курсов